<h2>Show Container</h2>

<ul>

  <li>
    <strong>Job no:</strong>
    <%= @container.job_no %>
  </li>

  <li>
    <strong>Job:</strong>
    <%= @container.job_id %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @container.name %>
  </li>

  <li>
    <strong>Status:</strong>
    <%= @container.status %>
  </li>

</ul>

<span><%= link "Edit", to: container_path(@conn, :edit, @container) %></span>
<span><%= link "Back", to: container_path(@conn, :index) %></span>
